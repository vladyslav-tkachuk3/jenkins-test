pipeline {
  agent {
    label "${params.deploy_environment}"
  }
  environment {
    IMAGE_REPO = 'vladyslavtkachuk3'
    IMAGE_NAME = 'ubuntu'
  }
  parameters {
    choice choices: ['buildin', 'dev', 'qa'], name: 'deploy_environment'
    choice choices: ['76', '74', '73', '65'], name: 'version'
  }
  stages {
    stage('pull image') {
      steps {
        sh "docker pull ${env.IMAGE_REPO}/${env.IMAGE_NAME}:${params.version}"
      }
    }
  }
}
