pipeline {
  agent {
    label "${params.deploy_environment}"
  }
  parameters {
    choice choices: ['buildin', 'dev', 'qa'], name: 'deploy_environment'
    choice choices: ['76', '74', '73', '65'], name: 'version'
  }
  stages {
    stage('build') {
      steps {
        sh "echo ${params.deploy_environment}"
        sh "echo ${params.version}"
      }
    }
    stage('pull image') {
      steps {
        sh "echo 'pull image'"
      }
    }
  }
}
