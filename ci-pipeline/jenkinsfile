#! groovy

properties([disableConcurrentBuilds()])

pipeline {
  agent {
    label 'buildin'
  }
  stages {
    stage('first step') {
      steps {
        echo 'hello world'
      }
    }
    stage('checkout') {
      steps {
        git branch: 'main', url: 'https://github.com/vladyslav-tkachuk3/jenkins-test.git'
      }
    }
  }
}
