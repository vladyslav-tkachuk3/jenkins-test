
#! groovy

properties([disableConcurrentBuilds()])

pipeline {
  agent {
    label 'buildin'
  }
  environment {
    BRANCH = 'feature-2'
  }
  stages {
    stage('clone repository') {
      steps {
        git branch: "${params.BRANCH}", url: 'https://github.com/vladyslav-tkachuk3/jenkins-test.git'
      }
    }
  }
}
